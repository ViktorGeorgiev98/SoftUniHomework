
        <main>
            <h1>Delete Cube</h1>
            <div class="form">
                <form action="/delete-cube-page/{{cubeToDelete._id}}" method="POST">
                    <label for="name">Name</label>
                    <input type="text" id="name" name="name" value="{{cubeToDelete.name}}" disabled="true" />
                    <label for="description">Description</label>
                    <textarea id="description" name="description" disabled="true" />{{cubeToDelete.description}}</textarea>
                    <label for="image">ImageUrl</label>
                    <input type="text" id="image" name="imageUrl" value="{{cubeToDelete.imageUrl}}" disabled="true" />
                    <label for="difficulty">Difficulty</label>
                    <select id="difficulty" name="difficultyLevel" value="{{cubeToDelete.difficultyLevel}}" disabled="true" />
                    <option>Difficulty level - {{cubeToDelete.difficultyLevel}}</option>
                    </select>
                    <input type="submit" value="Delete">
                </form>
            </div>
        </main>

